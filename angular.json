{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "dist": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/app",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "polyfills": "src/polyfills.ts",
                        "stylePreprocessorOptions": {
                            "includePaths": ["node_modules", "./projects/ama-sdk/src/lib/theming"]
                        },
                        "assets": [
                            "src/assets",
                            "src/app.config.json",
                            {
                                "input": "resources",
                                "output": "/resources",
                                "glob": "**/*"
                            },
                            {
                                "input": "src/app/process-editor/i18n",
                                "output": "/assets/process-editor/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "src/app/connector-editor/i18n",
                                "output": "/assets/connector-editor/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "projects/ama-sdk/i18n",
                                "output": "/assets/ama-sdk/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/@alfresco/adf-core/prebuilt-themes",
                                "output": "/assets/prebuilt-themes",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/@alfresco/adf-core/bundles/assets",
                                "output": "/assets",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                                "output": "/assets/monaco",
                                "glob": "**/*"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            ".tmp/bpmn-properties.css",
                            "projects/ama-sdk/src/lib/ama-sdk.module.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "dist:build",
                        "port": 4200,
                        "proxyConfig": "config/proxy.conf.js"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "dist:build:production"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-builders/jest:run",
                    "options": {
                        "configPath": "../jest.config.js"
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "dist:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**"],
                        "format": "stylish"
                    }
                }
            }
        },
        "adfdev": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/app",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.adfdev.json",
                        "polyfills": "src/polyfills.ts",
                        "stylePreprocessorOptions": {
                            "includePaths": ["../alfresco-ng2-components/lib", "./projects/ama-sdk/src/lib/theming"]
                        },
                        "assets": [
                            "src/assets",
                            "src/app.config.json",
                            "src/favicon.ico",
                            {
                                "input": "resources",
                                "output": "/resources",
                                "glob": "**/*"
                            },
                            {
                                "input": "src/app/process-editor/i18n",
                                "output": "/assets/process-editor/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "src/app/connector-editor/i18n",
                                "output": "/assets/connector-editor/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "projects/ama-sdk/i18n",
                                "output": "/assets/ama-sdk/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "dist/dev-temp",
                                "output": "/",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                                "output": "/assets/monaco",
                                "glob": "**/*"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            ".tmp/bpmn-properties.css",
                            "projects/ama-sdk/src/lib/ama-sdk.module.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "adfdev:build",
                        "port": 4200,
                        "proxyConfig": "config/proxy.conf.js"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "adfdev:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "adfdev:build"
                    }
                }
            }
        },
        "e2e": {
            "root": "e2e",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/app",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "polyfills": "src/polyfills.ts",
                        "stylePreprocessorOptions": {
                            "includePaths": ["node_modules", "./projects/ama-sdk/src/lib/theming"]
                        },
                        "assets": [
                            "src/assets",
                            "src/app.config.json",
                            {
                                "input": "resources",
                                "output": "/resources",
                                "glob": "**/*"
                            },
                            {
                                "input": "src/app/process-editor/i18n",
                                "output": "/assets/process-editor/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "src/app/connector-editor/i18n",
                                "output": "/assets/connector-editor/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "projects/ama-sdk/i18n",
                                "output": "/assets/ama-sdk/i18n",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/@alfresco/adf-core/prebuilt-themes",
                                "output": "/assets/prebuilt-themes",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/@alfresco/adf-core/bundles/assets",
                                "output": "/assets",
                                "glob": "**/*"
                            },
                            {
                                "input": "node_modules/ngx-monaco-editor/assets/monaco",
                                "output": "/assets/monaco",
                                "glob": "**/*"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            ".tmp/bpmn-properties.css",
                            "projects/ama-sdk/src/lib/ama-sdk.module.scss"
                        ],
                        "scripts": [],
                        "fileReplacements": [
                            {
                                "replace": "./src/app.config.json",
                                "with": "./e2e/app.config.json"
                            }
                        ]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "./e2e/protractor.conf.ts",
                        "devServerTarget": "e2e:serve"
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "e2e:build",
                        "proxyConfig": "config/proxy.conf.js",
                        "host": "localhost",
                        "port": 4200
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["e2e/tsconfig.e2e.json"],
                        "exclude": ["**/node_modules/**"],
                        "format": "stylish"
                    }
                }
            }
        },
        "ama-sdk": {
            "root": "projects/ama-sdk",
            "sourceRoot": "projects/ama-sdk/src",
            "projectType": "library",
            "prefix": "amasdk",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/ama-sdk/tsconfig.lib.json",
                        "project": "projects/ama-sdk/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "project": "projects/ama-sdk/ng-package.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/ama-sdk/tsconfig.lib.json"],
                        "exclude": ["**/node_modules/**"],
                        "format": "stylish"
                    }
                }
            }
        },
        "ama-testing": {
            "root": "projects/ama-testing",
            "sourceRoot": "projects/ama-testing/src",
            "projectType": "library",
            "prefix": "amatesting",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/ama-testing/tsconfig.lib.json",
                        "project": "projects/ama-testing/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "project": "projects/ama-testing/ng-package.prod.json"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/ama-testing/tsconfig.lib.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        }
    },
    "defaultProject": "dist",
    "schematics": {
        "@ngrx/schematics:component": {
            "prefix": "app",
            "styleext": "scss"
        },
        "@ngrx/schematics:directive": {
            "prefix": "app"
        }
    }
}
