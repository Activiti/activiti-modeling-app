        "adf-candidates": {
            "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
            "projectType": "library",
            "root": "projects/adf-candidates",
            "sourceRoot": "projects/adf-candidates",
            "prefix": "adf-candidates",
            "targets": {
                "test": {
                    "executor": "@nrwl/jest:jest",
                    "options": {
