        },
        "modeling-ce-process-editor": {
            "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
            "root": "projects/process-editor",
            "sourceRoot": "projects/process-editor/src",
            "projectType": "library",
            "prefix": "modeling-ce-process-editor",
            "targets": {
