                "<node_internals>/**/*.js"
            ],
            "internalConsoleOptions": "neverOpen",
            "envFile": "${workspaceFolder}/e2e/local.env",
            "env": {
                "BROWSER_RUN": "true",
                "BACKEND": "ACS"
            }
        },
